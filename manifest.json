{
  "manifest_version": 2,
  "name": "MDLink",
  "version": "0.1.1",
  "description": "Create a markdown link of the current page",
  "icons": {
    "48": "logo.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "logo.png"
    },
    "default_title": "Markdown Link Maker",
    "default_popup": "popup.html"
  },
  "permissions": ["clipboardRead", "tabs"]
}
