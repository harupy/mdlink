{
    "manifest_version": 2,
    "name": "Markdown Link Maker",
    "version": "1.0",

    "description": "Create a markdown link of the current page",
    "icons": {
        "48": "md_logo.png"
    },
    "browser_action": {
        "default_icon": {
            "48": "md_logo.png"
        },
        "default_title": "Markdown Link Maker",
        "default_popup": "popup.html"
    },
    "permissions":["clipboardRead","tabs"]
}